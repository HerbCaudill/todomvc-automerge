{
  "name": "todomvc-automerge",
  "version": "0.0.0",
  "license": "MIT",
  "author": "herb@devresults.com",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "postinstall": "cd node_modules/automerge && yarn && yarn build && cd ../.."
  },
  "dependencies": {
    "automerge": "git+https://@github.com/automerge/automerge.git#performance",
    "todomvc-app-css": "2",
    "classnames": "2",
    "react": "17",
    "react-dom": "17"
  },
  "devDependencies": {
    "@types/classnames": "2",
    "@types/react": "17",
    "@types/react-dom": "17",
    "@vitejs/plugin-react-refresh": "1",
    "typescript": "4",
    "vite": "2"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  }
}
